create table tasks (id_task serial primary key, task varchar(50), date_id references dates(id_date))
create table dates (id_date serial primary key, date date)



cursor = connection.cursor()
cursor.execute("create table dates (id_date serial primary key, date date)")
connection.commit()
cursor.close()
